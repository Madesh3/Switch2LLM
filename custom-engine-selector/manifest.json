{
  "manifest_version": 3,
  "name": "LLM Query Opener",
  "description": "Open queries with different LLMs from the address bar.",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "omnibox": {
    "keyword": "llm"
  }
}